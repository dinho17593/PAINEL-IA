const db = require('./database');
(async () => {
    try {
        const s = await db.getSettings();
        s.allowRegistrations = false;
        await db.saveSettings(s);
        console.log('â›” SUCESSO: Novos registros foram DESATIVADOS.');
        process.exit(0);
    } catch (e) { console.error(e); }
})();
